			alpha="1.0"
			copyright="National Park Service"
			subDomains="{ABCD}"
			urlTemplate="{npsParkPolygonsUrlTemplate}" />
		<esri:ArcGISDynamicMapServiceLayer
			id="units"
			visible="{unitsToggle.selected}"
			url="{resourceManager.getString('urls', 'HgDataUrl')}" >
